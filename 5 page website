    <script>
        // Accordion
        function myAccFunc() {
            var x = document.getElementById("demoAcc");
            if (x.className.indexOf("utg-show") == -1) {
                x.className += " utg-show";
            } else {
                x.className = x.className.replace(" utg-show", "");
            }
        }


        // Click on the " Jersey " link on page load to open the accordion for demo purposes
        document.getElementById("myBtn").click();


        // Open and close sidebar
        function utg_open() {
            document.getElementById("mySideBar").style.display = "block";
            document.getElementById("myOverlay").style.display = "block";
        }

    </script>

